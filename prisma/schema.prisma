generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("S7S_PRISMA_URL")
  relationMode = "prisma"
}

model User {
  id      String  @id @default(cuid())
  isAdmin Boolean @default(false)

  // Spotify auth stuff 
  spotifyAuthCode     String? @db.Text
  spotifyAuthState    String? @db.Text
  spotifyAccessToken  String? @db.Text
  spotifyRefreshToken String? @db.Text
}
